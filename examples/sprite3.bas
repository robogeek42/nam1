902 DATA $07, $1c, $38, $e0, $e0, $38, $1c, $07
903 DATA $ff, $ff, $ff, $ff, $ff, $ff, $ff, $ff
904 DATA $18, $3c, $66, $c3, $c3, $66, $3c, $18
1000 REM Load sprite
1020 BADDR=VARPTR(BUF(0))
1030 FOR I=0 TO 7
1040 READ D
1050 POKE BADDR+I, D
1060 NEXT
1080 SPR L BADDR, P
1090 SPR P S,P
1100 RETURN
