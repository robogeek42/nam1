10 MODE 1
20 COL 4,15
30 DIM BUF(2)
100 S=0:P=0:A=0:Y=0
110 I=0:D=0:M=0

115 RESTORE 900
120 SPR T 0
130 FOR S=1 TO 5
140 P=S
150 GOSUB 1000
160 NEXT S

210 SPR E 0
220 SPR N 5
230 SPR X 5,-32,-32
240 FOR S=1 TO 4
250 SPR C S,3+S
260 SPR X S,20+S*20,96
270 NEXT S
280 SPR C 5,8
290 FOR I=64 TO 132 STEP 1
300 SPR X 5,20+5*20,I
310 DELAY 60
315 GOSUB 800
320 NEXT I
330 FOR I=80 TO 0 STEP -1
340 SPR X 4, 20+I, 96
350 DELAY 80
360 GOSUB 800
380 NEXT I

699 END
700 CURS 0,0 : PRINT "COLLISION" : RETURN
710 CURS 0,0 : PRINT "          " : RETURN
720 CURS 0,1 : PRINT "FIFTH ";(SS AND $1F) : RETURN
730 CURS 0,1 : PRINT "          " : RETURN

800 SS=SSTATUS
810 IF (SS AND $20)>0 THEN GOSUB 700 ELSE GOSUB 710
820 IF (SS AND $40)>0 THEN GOSUB 720 ELSE GOSUB 730
830 RETURN

900 DATA $3c, $7e, $5a, $7e, $24, $3c, $66, $c3
